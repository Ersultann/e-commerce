 $(document).ready(function() {
	console.log('start');
	$('#qty-plus').on('click', function() {
		var qty = $('#input-quantity').val();
		if (qty > 0 && qty < 99){
			qty = parseInt(qty) + 1;
		} else {
			qty = 1;
		}
		$('#input-quantity').val(qty);
	});
	$('#qty-minus').on('click', function() {
		var qty = $('#input-quantity').val();
		if (qty > 1 && qty < 100){
			qty = parseInt(qty) - 1;
		} else {
			qty = 1;
		}
		$('#input-quantity').val(qty);
	});
 });</script>
